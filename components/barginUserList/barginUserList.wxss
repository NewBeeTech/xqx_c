/* userlistx */
.bargin_user_content {
  /* padding: 38rpx; */
}
.bargin_user_list {
  display: flex;
  justify-content: center;
  align-items: center;
  margin-bottom: 30rpx;
}

.bargin_header_line {
  width: 128rpx;
  height: 1rpx;
  border-top: 1rpx solid #dadada;
}

.bargin_header_title {
  font-size: 30rpx;
  color: #333;
  margin: 0 30rpx;
}

.bargin_user_item {
  height: 100rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  margin: 0 38rpx;
  font-size: 26rpx;
  color: #333;
  border-bottom: 1rpx solid #dadada;
}

.bargin_user_item > view > image {
  width: 70rpx;
  height: 70rpx;
  border-radius: 50%;
  margin: 0 18rpx;
}

.bargin_user_item > view {
  display: flex;
  justify-content: flex-start;
  align-items: center;
}
