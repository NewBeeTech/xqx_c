.download-app-icon {
  width: 150rpx;
  height: 150rpx;
  margin-top: 150rpx;
}
.download-app-desc {
  font-size: 26rpx;
  max-width: 500rpx;
  text-align: center;
  margin: 50rpx 0;
  color: grey;
  word-wrap: break-word;
  word-break: break-all;
}
.download-app-btn {
  width: 600rpx;
  text-align: center;
  background-color: #FEA401;
  height: 90rpx;
  border-radius: 10rpx;
  color: #f0f0f0;
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 25rpx;
}
.container {
  display: flex;
  flex-direction: column;
  align-items: center;
}
